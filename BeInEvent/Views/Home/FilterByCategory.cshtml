@using BeInEvent.Models
@model Category
@{ 
    IEnumerable<Event> getevent = ViewBag.every as IEnumerable<Event>;
}
    <table class="table table-bordered table-condensed table-hover">
        <tr>
            <th>Event Name</th>
            <th>Details</th>

        </tr>

        @foreach (var events in getevent)
        {
            <tr>
                <td>@events.EventName</td>
                <td>@Html.ActionLink("More", "eachevent","Events", new { id = events.EventID }, new { @class = "btn-def bnt-2" })</td>

            </tr>
        }



    </table>
